{
    "name": "academe/omnipay-payone",
    "type": "library",
    "description": "PAYONE gateway driver for the Omnipay PHP payment processing library",
    "keywords": [
        "payone",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/academe/omnipay-payone",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Judge",
            "email": "jason@academe.co.uk"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-sagepay/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Payone\\" : "src/"
        }
    },
    "require": {
        "omnipay/common": "~3.0",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
